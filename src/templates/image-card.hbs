{{#each this}}
<div class="photo-card">
  <a class="image-link" href="{{largeImageURL}}">
    <img
      class="gallery-image"
      src="{{webformatURL}}"
      alt="{{tags}}"
      loading="lazy"
      height="240px"
    />
    <div class="info">
      <p class="info-item">
        <b>Likes</b>
        {{likes}}
      </p>
      <p class="info-item">
        <b>Views</b>
        {{views}}
      </p>
      <p class="info-item">
        <b>Comments</b>
        {{comments}}
      </p>
      <p class="info-item">
        <b>Downloads</b>
        {{downloads}}
      </p>
    </div>
  </a>
</div>
{{/each}}